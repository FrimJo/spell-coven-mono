{
  "name": "@repo/web",
  "private": true,
  "type": "module",
  "scripts": {
    "with-env": "dotenv -e ../../.env.development -e ../../.env.development.local -e .env.development -e .env.development.local --",
    "dev": "bun run with-env -- vite dev",
    "build": "bun run with-env -- vite build",
    "serve": "bun run with-env -- vite preview",
    "test": "bun run vitest run",
    "format": "bun run prettier --check . --ignore-path ./.prettierignore --ignore-path ./.gitignore --ignore-path ../../.gitignore",
    "format:fix": "bun run prettier --check . --ignore-path ./.prettierignore --ignore-path ./.gitignore --ignore-path ../../.gitignore --write",
    "lint": "bun run eslint . --max-warnings 0",
    "lint:fix": "bun run eslint . --max-warnings 0 --fix",
    "check-types": "bun run tsc --noEmit",
    "e2e": "bun run playwright test",
    "e2e:ui": "bun run playwright test --ui"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.7.5",
    "@repo/discord-gateway-worker": "workspace:*",
    "@repo/discord-integration": "workspace:*",
    "@repo/ui": "workspace:*",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/zod-adapter": "^1.133.3",
    "@techstark/opencv-js": "4.11.0-release.1",
    "cross-fetch": "^4.1.0",
    "crossws": "^0.4.1",
    "discord-api-types": "^0.37.97",
    "jose": "^5.9.6",
    "lucide-react": "^0.487.0",
    "onnxruntime-web": "^1.23.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^4.0.0",
    "sonner": "^2.0.3",
    "tailwindcss": "^4.0.6",
    "vite-plugin-mkcert": "^1.17.9",
    "vite-tsconfig-paths": "^5.1.4",
    "ws": "^8.18.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@playwright/test": "catalog:",
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/ws": "^8.5.12",
    "@vitejs/plugin-react": "^4.3.4",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.34.0",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "jsdom": "^26.0.0",
    "playwright": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "catalog:",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^4.2.4"
  },
  "prettier": "@repo/prettier-config"
}
